<%= simple_form_for(@tracking) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :area %>
    <%= f.input :usage %>
    <%= f.input :value %>
    <%= f.input :unit %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
